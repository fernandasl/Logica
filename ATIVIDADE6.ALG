Algoritmo "atividade6"

Var

   velocidade, m, vm : real

Inicio

  escreval("Informe a velocidade do veículo: ")
  leia(velocidade)

  vm <- 434.4 / velocidade
   m <- velocidade / 3.6
   
   escreval("O tempo de viagem de São Paulo até o Rio de Janeiro é de: ", vm, "hs")
   escreval("Velocidade em m/s: ", m)

Fimalgoritmo