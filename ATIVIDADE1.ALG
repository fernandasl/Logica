Algoritmo "atividade1"
Var

 nome1, nome2, espaco : caracter
 soma : caracter


Inicio
      escreval("Qual é o seu primeiro nome?")
      leia(nome1)
      escreval("Qual é o seu sobrenome?")
      leia(nome2)
      
      soma <- nome1 + nome2
      
      escreval("seu nome completo é: ", soma)

      

Fimalgoritmo